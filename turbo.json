{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "build/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    }
  }
}
